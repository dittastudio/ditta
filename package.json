    "@vueuse/core": "^11.2.0",
    "gsap": "^3.12.5",
    "swiper": "^11.1.14",
    "vee-validate": "^4.14.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@michaelpumo/screen": "^0.0.18",
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/sitemap": "^6.1.4",
    "@nuxtjs/stylelint-module": "^5.2.0",
